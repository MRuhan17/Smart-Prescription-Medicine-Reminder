{
    "expo": {
        "name": "Smart Prescription Reminder",
        "slug": "smart-prescription-reminder",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/icon.png",
        "userInterfaceStyle": "light",
        "splash": {
            "image": "./assets/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "assetBundlePatterns": [
            "**/*"
        ],
        "ios": {
            "supportsTablet": true,
            "bundleIdentifier": "com.smartprescription.reminder"
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/adaptive-icon.png",
                "backgroundColor": "#ffffff"
            },
            "package": "com.smartprescription.reminder"
        },
        "web": {
            "favicon": "./assets/favicon.png"
        },
        "extra": {
            "apiBaseUrl": "http://localhost:8000"
        }
    }
}