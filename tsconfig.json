{
    "extends": "expo/tsconfig.base",
    "compilerOptions": {
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "resolveJsonModule": true,
        "jsx": "react-native",
        "lib": [
            "ES2015",
            "ES2017",
            "ES2020"
        ],
        "moduleResolution": "node",
        "allowSyntheticDefaultImports": true
    }
}